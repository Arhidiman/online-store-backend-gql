    "rootDir": "./",
"noEmit": true,
      "allowImportingTsExtensions": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["services/**/*", "gateway.ts"]
}
