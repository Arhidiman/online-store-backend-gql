{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "rootDir": "./",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    },
    "include": ["./**/*.ts", "services/productsService.ts"],
    "exclude": ["node_modules", "dist"]
  }
  